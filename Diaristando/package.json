{
  "name": "diaristando",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "eas-build-post-install": "npx tailwindcss -i ./src/styles/global.css -o ./node_modules/.cache/nativewind/global.css.native.css",
  "postinstall": "npx tailwindcss -i ./src/styles/global.css -o ./node_modules/.cache/nativewind/global.css.native.css",
  "scripts": {
    "start": "expo start",
    "start:wsl": "REACT_NATIVE_PACKAGER_HOSTNAME=$(node get_network_local_ip.js) expo start -c",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,md}'",
    "lint": "eslint ."
  },
  "dependencies": {
    "@expo-google-fonts/roboto": "^0.2.3",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/native": "^6.1.17",
    "expo": "~51.0.14",
    "expo-dev-client": "~4.0.20",
    "expo-font": "~12.0.7",
    "expo-status-bar": "~1.12.1",
    "i18next": "^23.12.1",
    "react": "18.2.0",
    "react-i18next": "^14.1.3",
    "react-native": "0.74.2",
    "nativewind": "4.0.35",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "tailwindcss": "^3.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-universe": "^13.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "typescript": "^5.1.3"
  },
  "private": true
}
